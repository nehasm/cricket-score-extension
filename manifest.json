{
    "manifest_version": 3,
    "name": "My Axios Extension",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["contentScripts.js"]
        }
      ],
    "web_accessible_resources": [
      {
        "resources": ["dist/bundle.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  